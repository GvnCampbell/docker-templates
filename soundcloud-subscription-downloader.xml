  <Config Name="config"                    Target="/config"                   Default="" Mode="rw" Description="Container Path: /config"                       Type="Path"     Display="always" Required="true" Mask="false"></Config>
  <Config Name="audio"                     Target="/audio"                    Default="" Mode="rw" Description="Container Path: /audio"                        Type="Path"     Display="always" Required="true" Mask="false"></Config>
  <Config Name="temp"                      Target="/temp"                     Default="" Mode="rw" Description="Container Path: /temp"                         Type="Path"     Display="always" Required="true" Mask="false"></Config>
