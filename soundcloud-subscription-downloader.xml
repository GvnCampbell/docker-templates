  <Config Name="LOG_FILE"                  Target="LOG_FILE"                  Default="" Mode=""   Description="Container Variable: LOG_FILE"                  Type="Variable" Display="always" Required="true" Mask="false">/config/soundcloud-subscription-downloader.log</Config>
